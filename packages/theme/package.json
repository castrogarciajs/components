{
  "name": "@svelteui-org/theme",
  "version": "1.0.0",
  "description": "Theme for svelte UI",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "type": "module",
  "scripts": {
    "build.dev": "tsup src --format cjs,esm --dts --watch",
    "build": "tsup src --format cjs,esm --dts",
    "lint": "tsc --notEmit"
  },
  "keywords": [
    "theme",
    "theming",
    "design",
    "ui",
    "components",
    "classNames",
    "css"
  ],
  "files": [
    "dist",
    "config.js",
    "config.d.ts",
    "plugin.js",
    "plugin.d.ts",
    "colors.js",
    "colors.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "5.2.2",
    "tsup": "7.2.0"
  },
  "peerDependencies": {
    "tailwindcss": "*"
  }
}
