{
  "name": "@qwikui-org/ui",
  "version": "0.2.3",
  "description": "Qwik ui components",
  "main": "lib/index.js",
  "types": "lib-types/index.d.ts",
  "style": "./styles.css",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./lib-types/index.d.ts"
    },
    "./styles.css": {
      "import": "./styles.css",
      "require": "./styles.css",
      "default": "./styles.css"
    }
  },
  "files": [
    "lib",
    "lib-types",
    "styles.css"
  ],
  "type": "module",
  "scripts": {
    "changeset.publish": "pnpm run build && pnpm run build.types && changeset publish",
    "changeset.version": "changeset version",
    "build.types": "tsc --emitDeclarationOnly",
    "lint.types": "tsc --noEmit",
    "fmt.check": "prettier --check .",
    "build": "esbuild src/index.ts --bundle --format=esm --outfile=lib/index.js --minify --sourcemap",
    "lint": "eslint --ext .ts,.tsx .",
    "fmt": "prettier --write --cache ."
  },
  "keywords": [],
  "author": "castrogarciajs",
  "license": "MIT",
  "dependencies": {
    "@builder.io/qwik": "1.2.16"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.2",
    "@types/eslint": "8.44.6",
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint-plugin-qwik": "1.2.15",
    "esbuild": "0.19.5",
    "eslint": "8.52.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "@builder.io/qwik": "1.2.16"
  }
}
