{
  "name": "@qwikui-org/ui",
  "version": "0.2.0",
  "description": "Create a reusable Qwik component library",
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "style": "./styles.css",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    },
    "./styles.css": {
      "import": "./styles.css",
      "require": "./styles.css",
      "default": "./styles.css"
    }
  },
  "files": [
    "lib",
    "lib-types",
    "styles.css"
  ],
  "type": "module",
  "private": false,
  "scripts": {
    "changeset.publish": "pnpm run lint && pnpm run test && pnpm run build.publish && changeset publish",
    "changeset.version": "changeset version",
    "build.publish": "pnpm run build && pnpm run build.css",
    "build.types": "tsc --emitDeclarationOnly",
    "lint.types": "tsc --noEmit",
    "fmt.check": "prettier --check .",
    "build.lib": "vite build --mode lib",
    "build.css": "esbuild --bundle ./src/theme/index.css --outfile=./styles.css --minify",
    "build": "qwik build",
    "start": "vite --open --mode ssr",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "echo \"No test specified\" && exit 0",
    "qwik": "qwik",
    "dev": "vite --mode ssr",
    "fmt": "prettier --write --cache .",
    "ci": "pnpm run fmt.check && pnpm run lint && pnpm run test && pnpm run build"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.15",
    "@changesets/cli": "2.26.2",
    "@types/eslint": "8.44.4",
    "@types/node": "20.8.4",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "autoprefixer": "10.4.16",
    "esbuild": "0.19.5",
    "eslint": "8.51.0",
    "eslint-plugin-qwik": "1.2.15",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "undici": "5.26.0",
    "vite": "4.4.11",
    "vite-tsconfig-paths": "4.2.1"
  },
  "peerDependencies": {
    "@builder.io/qwik": "1.2.15"
  },
  "author": "castrogarciajs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/castrogarciajs/qwikui.git",
    "directory": "qwikui"
  }
}
