{
  "name": "@qwik-org/ui",
  "version": "0.0.1",
  "description": "Create a reusable Qwik component library",
  "main": "./dist/index.qwik.mjs",
  "qwik": "./dist/index.qwik.mjs",
  "types": "./dist-dist/index.d.ts",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.qwik.mjs",
      "require": "./dist/index.qwik.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build.types": "tsc --emitDeclarationOnly && pnpm --filter=./doc build.types",
    "fmt.check": "prettier --check .",
    "build.lib": "vite build --mode lib",
    "start.doc": "pnpm --filter=./doc start",
    "dev.doc": "pnpm --filter=./doc dev",
    "build": "qwik build && pnpm --filter=./doc build",
    "start": "vite --open --mode ssr",
    "lint": "eslint \"src/**/*.ts*\" \"doc/**/*.tsx*\" --fix",
    "test": "echo \"No test specified\" && exit 0",
    "qwik": "qwik",
    "dev": "vite --mode ssr",
    "fmt": "prettier --write --cache ."
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.15",
    "@types/eslint": "8.44.4",
    "@types/node": "20.8.4",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "eslint": "8.51.0",
    "eslint-plugin-qwik": "1.2.15",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "undici": "5.26.0",
    "vite": "4.4.11",
    "vite-tsconfig-paths": "4.2.1"
  }
}
